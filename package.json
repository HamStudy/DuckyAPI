{
  "name": "ducky-api",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --color --config webpack.config.prod.js",
    "build:dev": "webpack --color --config webpack.config.dev.js",
    "start": "wait-on dist/main.js && node dist/main",
    "start:dev": "wait-on dist/main.js && nodemon --watch config/development.env --exec \"node --unhandled-rejections=strict dist/main\"",
    "start:debug": "wait-on dist/main.js && nodemon --watch config/development.env --exec \"node --inspect dist/main\"",
    "buildstart": "npm-run-all --silent --print-label clean build start",
    "buildstart:dev": "npm-run-all --silent --print-label clean --parallel build:dev start:dev",
    "buildstart:debug": "npm-run-all --silent --print-label clean --parallel build:dev start:debug",
    "openapi": "npm-run-all --silent --print-label clean build openapi:generate",
    "openapi:generate": "node dist/generate-openapi",
    "lint": "npm-run-all --silent --print-label lint:tsc lint:eslint",
    "lint:fix": "npm-run-all --silent --print-label lint:tsc lint:eslintfix",
    "lint:tsc": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "lint:eslint": "eslint --color --max-warnings 0 ./ --ext ts",
    "lint:eslintfix": "eslint --fix --color --max-warnings 0 ./ --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/bull": "0.1.2",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.0",
    "bcrypt": "^5.0.0",
    "bull": "^3.18.0",
    "bull-arena": "^3.2.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "commander": "^6.0.0",
    "duckypanel": "0.0.5",
    "express-basic-auth": "^1.2.0",
    "generate-password": "^1.5.1",
    "helmet": "^4.1.0",
    "json5": "^2.2.0",
    "mongodb": "^4.2.1",
    "nanoid": "^3.1.12",
    "nestjs-console": "^3.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "snyk": "^1.381.1",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.4.2",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.1",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "axios": "^0.19.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.4.1",
    "js-yaml": "^3.14.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "openapi-snippet": "^0.9.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "~4.5.0",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2",
    "webpack-node-externals": "^2.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
